---
import { useLink } from '@/features/router/useLink';
import { getProps } from '@/features/utils/getProps';

export interface Props {
  to: string;
  [key: string]: any;
}

const { to, children, ...restProps } = getProps<Props>(Astro);
const url = useLink(Astro)(to);

---
<a {...restProps} href={url}>
  <slot />
</a>
