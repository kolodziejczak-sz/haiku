---
import { getProps } from '@/features/utils/getProps';
import RawLayout from '@/layouts/RawLayout.astro';
import Header from '@/components/Header.astro'

export interface Props {
  className?: string;
  title?: string;
};

const { title, className } = getProps<Props>(Astro);

---
<RawLayout title={title}>
  <Header />
  <main class={className}>
    <slot />
  </main>
</RawLayout>